<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".streamOfPraise17.menu17">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="返回"
                android:id="@+id/btnMenu17Previous"
                android:textSize="18sp"
                android:background="#AAAAAA" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1.將天敞開"
                android:id="@+id/song1701將天敞開"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#DDDDDD"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="0dp" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="2.日日夜夜"
                android:id="@+id/song1702日日夜夜"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="#FFFFFF"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3.配得頌揚"
                android:id="@+id/song1703配得頌揚"
                android:layout_alignParentStart="true"
                android:background="#DDDDDD"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:layout_below="@+id/song1702日日夜夜"
                android:layout_alignParentLeft="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="4.活著為要敬拜祢"
                android:id="@+id/song1704活著為要敬拜祢"
                android:layout_alignParentStart="true"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:background="#FFFFFF"
                android:layout_below="@+id/song1703配得頌揚"
                android:layout_alignParentLeft="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="5.新耶路撒冷"
                android:id="@+id/song1705新耶路撒冷"
                android:layout_alignParentStart="true"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#DDDDDD"
                android:layout_below="@+id/song1704活著為要敬拜祢"
                android:layout_alignParentLeft="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="6.一同起舞"
                android:id="@+id/song1706一同起舞"
                android:layout_alignParentStart="true"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#FFFFFF"
                android:layout_below="@+id/song1705新耶路撒冷"
                android:layout_alignParentLeft="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/song1707十架的愛"
                android:layout_alignParentStart="true"
                android:text="7.十架的愛"
                android:layout_below="@+id/song1706一同起舞"
                android:layout_alignParentLeft="true"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#DDDDDD" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="8.救贖的恩典"
                android:id="@+id/song1708救贖的恩典"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/song1707十架的愛"
                android:layout_alignParentLeft="true"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#FFFFFF" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="9.為耶路撒冷祈禱"
                android:id="@+id/song1709為耶路撒冷祈禱"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/song1708救贖的恩典"
                android:layout_alignParentLeft="true"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#DDDDDD" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="10.我要順服"
                android:id="@+id/song1710我要順服"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/song1709為耶路撒冷祈禱"
                android:layout_alignParentLeft="true"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#FFFFFF" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="11.自由地歌唱"
                android:id="@+id/song1711自由地歌唱"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/song1710我要順服"
                android:layout_alignParentLeft="true"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#DDDDDD" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="12.就是這個時刻"
                android:id="@+id/song1712就是這個時刻"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#FFFFFF" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="13.我已得自由"
                android:id="@+id/song1713我已得自由"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#DDDDDD" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="14.同心高舉十架"
                android:id="@+id/song1714同心高舉十架"
                android:gravity="center_vertical"
                android:textSize="18sp"
                android:background="#FFFFFF" />

        </LinearLayout>
    </ScrollView>

</RelativeLayout>
